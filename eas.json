{
    "cli": {
      "version": ">= 0.52.0"
    },
    "build": {
      "development": {
        "env": {
          "EXPO_PUBLIC_API_URL": "http://localhost:8082"
        },
        "developmentClient": true,
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        },
        "ios": {
          "simulator": true
        }
      },
      "preview": {
        "env": {
          "EXPO_PUBLIC_API_URL": "https://autonomos-app.onrender.com"
        },
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        }
      },
      "production": {
        "env": {
          "EXPO_PUBLIC_API_URL": "https://autonomos-app.onrender.com"
        }
      }
    }
  }